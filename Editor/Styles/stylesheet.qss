/* Top widget of the property tree */
QPropertyTree
{
	qproperty-collapsedIcon: url(icons:General/Pointer_Right.ico);
	qproperty-expandedIcon: url(icons:General/Pointer_Down_Expanded.ico);
	qproperty-dragHandleIcon: url(icons:General/Drag_Handle.ico);
}

/* Inner widget of the property tree. Contains rows and possibly other FormWidgets inside.
Use the nesting property value to style differently for different nesting levels.*/
PropertyTree2--CFormWidget 
{
	qproperty-margins: rect(0 0 0 0);
	alternate-background-color: rgb(77,77,77);
	font-size: 12px;
	qproperty-spacing: 0;
	qproperty-iconSize: 16;
	qproperty-minRowHeight: 24;
	qproperty-nestingIndent: 24;
}

PropertyTree2--CFormWidget[nesting="1"] 
{
	qproperty-margins: rect(0 2 0 1);
}

PropertyTree2--CFormWidget[nesting="0"] 
{
	color:white;
	qproperty-groupBorderColor: rgb(128,128,128);
	qproperty-groupBorderWidth: 1;
	qproperty-margins: rect(0 0 4 0);
}

/* For e.g. array "add" button to have the same height as regular rows */
PropertyTree2--CFormWidget QToolButton 
{
	max-width: 20;
	max-height: 20;
	icon-size: 20px;
}

