float4 EncodeHDR(float3 Color)
{
	float fMax = max(1.f / 4.f, max( max(Color.r, Color.g), Color.b));
	return float4(Color.xyz / fMax, fMax / 4.f);
}

float3 DecodeHDR(float4 Color)
{
	Color.rgb = Color.rgb * (Color.a * 4.f);
	return Color.rgb;
}

